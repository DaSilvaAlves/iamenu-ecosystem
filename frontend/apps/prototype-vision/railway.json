{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm install && npm run build --workspace=prototype-vision"
  },
  "deploy": {
    "startCommand": "npx serve -s dist",
    "restartPolicyType": "ON_FAILURE",
    "rootDirectory": "frontend/apps/prototype-vision"
  }
}
