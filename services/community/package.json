{
  "name": "@iamenu/community-api",
  "version": "1.0.0",
  "description": "iaMenu Community Hub API - Forums, Groups, Networking",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dotenv -e .env tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build -t iamenu-community-api .",
    "docker:run": "docker run -p 3001:3001 --env-file .env iamenu-community-api"
  },
  "keywords": [
    "iamenu",
    "community",
    "forum",
    "restaurants",
    "networking"
  ],
  "author": "Eurico Alves <euricojsalves@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/multer": "^2.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
